{
 "name": "SplashDesktop",
 "version": "0.1.0",
 "main": "app.js",
 "license": "MIT",
 "description": "Splah Cli desktop tool",
 "author": "Rawnly <fedevitale99@gmail.com> (rawnly.com)",
 "dependencies": {
  "about-window": "^1.6.3",
  "command-exists": "^1.2.2",
  "electron-is": "^2.4.0",
  "electron-lets-move": "^0.0.5",
  "electron-main-notification": "^1.0.1",
  "electron-settings": "^3.1.1",
  "execa": "^0.7.0",
  "first-run": "^1.2.0",
  "got": "^7.0.0",
  "mkdirp": "^0.5.1",
  "normalize-url": "^1.9.1",
  "simple-download": "^0.3.1",
  "titlebar": "^1.4.0",
  "url-regex": "^4.1.1",
  "wallpaper": "^2.4.0"
 },
 "devDependencies": {
  "electron": "^1.6.11",
  "electron-builder": "^18.6.2",
  "electron-packager": "^8.7.1"
 },
 "scripts": {
  "package-macos": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=false --out=dist",
	"build": "build"
 },
 "build": {
  "appId": "com.electron.splash-desktop",
  "productName": "Splash Desktop",
  "copyright": "Copyright Â© 2017 ${author}",
  "asar": true,
  "compression": "store",
  "artifactName": "${productName}-${version}.${ext}",
  "mac": {
   "category": "public.app-category.photography",
   "target": "dmg"
  }
 }
}
